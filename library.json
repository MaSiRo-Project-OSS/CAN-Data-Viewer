{
    "name": "CAN_Data_Viewer",
    "version": "0.2",
    "description": "View CAN data in a browser",
    "keywords": "M5Stack, ATOM Lite",
    "homepage": "https://github.com/MaSiRo-Project-OSS/CAN-Data-Viewer",
    "authors": {
        "name": "MaSiRo Project.",
        "email": "masiro.to.akari@gmail.com",
        "url": "https://github.com/MaSiRo-Project-OSS/CAN-Data-Viewer"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MaSiRo-Project-OSS/CAN-Data-Viewer.git"
    },
    "license": "MIT",
    "frameworks": "arduino",
    "platforms": "espressif32",
    "dependencies": {
        "coryjfowler/mcp_can": "^1.5.0",
        "miwagner/ESP32CAN": "~0.0.1"
    },
    "headers": [
        "can_data_viewer.hpp",
        "can_data_viewer_info.hpp"
    ],
    "examples": [
        {
            "name": "PlatformIO",
            "base": "examples/PlatformIO",
            "files": [
                "platformio.ini",
                "include/examples.h",
                "src/main.c",
                "README"
            ]
        },
        {
            "name": "Arduino",
            "base": "examples/can_data_viewer",
            "files": [
                "can_data_viewer.ino"
            ]
        }
    ],
    "build": {
        "includeDir": "include",
        "srcDir": "src",
        "flags": [
            "-DNO_OUTPUT_MESSAGE_FOR_SERIAL=1",
            "-DNO_LIB_CAN_DRIVER=0"
        ]
    }
}