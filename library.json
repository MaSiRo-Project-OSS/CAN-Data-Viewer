{
    "name": "CANDataViewer",
    "version": "0.2.1",
    "description": "View CAN data in a browser",
    "keywords": "M5Stack, ATOM Lite, CAN, WebServer",
    "homepage": "https://github.com/MaSiRoProjectOSS/CANDataViewer",
    "authors": {
        "name": "MaSiRo Project.",
        "email": "masiro.to.akari@gmail.com",
        "url": "https://github.com/MaSiRoProjectOSS/CANDataViewer"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MaSiRoProjectOSS/CANDataViewer.git"
    },
    "license": "MIT",
    "frameworks": "arduino",
    "platforms": "espressif32",
    "dependencies": {
        "coryjfowler/mcp_can": "^1.5.0",
        "miwagner/ESP32CAN": "~0.0.1"
    },
    "headers": "can_data_viewer.hpp",
    "examples": [
        {
            "name": "CanDataViewer_basic",
            "base": "examples/CanDataViewer_basic",
            "files": [
                "CanDataViewer_basic.ino"
            ]
        },
        {
            "name": "CanDataViewer_PlatformIO",
            "base": "examples/CanDataViewer_PlatformIO",
            "files": [
                "src/main.cpp",
                "platformio.ini"
            ]
        },
        {
            "name": "CanDataViewer_FullFunction",
            "base": "examples/CanDataViewer_FullFunction",
            "files": [
                "CanDataViewer_FullFunction.ino"
            ]
        },
        {
            "name": "CanDataViewer_M5Atom",
            "base": "examples/CanDataViewer_M5Atom",
            "files": [
                "CanDataViewer_M5Atom.ino"
            ]
        }
    ],
    "build": {
        "includeDir": "include",
        "srcDir": "src"
    }
}